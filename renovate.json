{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base"
  ],
  "matchUpdateTypes": [
    "minor",
    "patch",
    "pin"
  ],
  "automerge": true,
  "automergeType": "pr",
  "platformAutomerge": true,
  "ignoreTests": true,
  "commitMessage": "chore(deps): update dependencies",
  "ignoreDeps": [
    "eslint",
    "typescript"
  ]
}

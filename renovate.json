{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base"
  ],
  "automerge": true,
  "automergeType": "pr",
  "commitMessage": "chore(deps): update dependencies",
    "labels": [
        "dependencies"
    ],
  "ignoreDeps": [
    "eslint",
    "typescript"
  ],
  "packageRules": [
    {
      "groupName": "dependencies",
      "automerge": true
    }
  ]
}
